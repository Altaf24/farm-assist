{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "cache-control": "s-maxage=1, stale-while-revalidate"
      }
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}